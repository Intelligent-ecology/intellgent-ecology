<view><view class="location"><block wx:for="{{userInfo}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="content"><view class="location_top"><view class="t_left"><text class="name">{{item.name+"，"+item.phone}}</text><text class="address">{{item.address}}</text></view><view class="t_right"><van-icon class="cross" vue-id="{{'1-'+index}}" name="cross" data-com-type="wx" bind:__l="__l"></van-icon><view><block wx:if="{{curIndex1==index}}"><van-icon vue-id="{{'2-'+index}}" name="success" data-com-type="wx" bind:__l="__l"></van-icon></block><van-button class="btn1" vue-id="{{'3-'+index}}" data-index="{{index}}" size="mini" type="default" wx:else data-event-opts="{{[['^tap',[['use']]]]}}" data-com-type="wx" bind:tap="__e" bind:__l="__l" vue-slots="{{['default']}}">使用</van-button></view></view></view><view class="location_bottom"><view class="b_left"><icon class="{{[item.def?'iconfont icon-xuanzhong-copy':'iconfont icon-xuanzhongyuandian']}}" data-index="{{index}}" data-event-opts="{{[['tap',[['ctrl',['$event']]]]]}}" bindtap="__e"></icon><text class="{{[item.def?'red':'']}}">{{item.def?text:text1}}</text></view><view class="b_right"><view class="edit">编辑</view><view class="topping" data-index="{{index}}" data-event-opts="{{[['tap',[['toTop',['$event']]]]]}}" bindtap="__e">{{item.top?top:top1}}</view></view></view></view></block></block><van-button class="button" vue-id="4" size="large" type="danger" data-event-opts="{{[['^tap',[['add']]]]}}" data-com-type="wx" bind:tap="__e" bind:__l="__l" vue-slots="{{['default']}}"><image src="../../static/img/add.png"></image><text>添加新收货地址</text></van-button><van-popup vue-id="5" show="{{show}}" data-event-opts="{{[['^close',[['onClose']]]]}}" data-com-type="wx" bind:close="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="popup_content"><view class="popup_top"><text>添加新收货地址</text><van-icon class="cross1" vue-id="{{('6')+','+('5')}}" name="cross" size="20px" data-com-type="wx" bind:__l="__l"></van-icon></view><view class="namePhone"><van-cell-group vue-id="{{('7')+','+('5')}}" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><van-field vue-id="{{('8')+','+('7')}}" value="{{ value }}" placeholder="收货姓名" border="{{ false }}" data-event-opts="{{[['^change',[['onChange']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l"></van-field></van-cell-group><van-cell-group vue-id="{{('9')+','+('5')}}" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><van-field vue-id="{{('10')+','+('9')}}" value="{{ value }}" placeholder="电话" border="{{ false }}" data-event-opts="{{[['^change',[['onChange']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l"></van-field></van-cell-group></view><view class="choose"><text>选择地区</text><van-icon class="arrow" vue-id="{{('11')+','+('5')}}" name="arrow" size="16px" data-com-type="wx" bind:__l="__l"></van-icon></view><view class="detailed"><van-cell-group vue-id="{{('12')+','+('5')}}" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><van-field vue-id="{{('13')+','+('12')}}" value="{{ value }}" placeholder="详细地址（如街道、小区、乡镇、村）" border="{{ false }}" data-event-opts="{{[['^change',[['onChange']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l"></van-field></van-cell-group></view><van-button class="save" vue-id="{{('14')+','+('5')}}" size="large" type="default" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}">保存</van-button></view></van-popup></view></view>