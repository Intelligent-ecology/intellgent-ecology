<view><view class="location"><block wx:for="{{userInfo}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="content"><view class="location_top"><view class="t_left"><text class="name">{{item.name+"，"+item.phone}}</text><text class="address">{{item.address}}</text></view><view class="t_right"><van-icon class="cross" vue-id="{{'1-'+index}}" name="cross" data-com-type="wx" bind:__l="__l"></van-icon><view><block wx:if="{{curIndex1==index}}"><van-icon vue-id="{{'2-'+index}}" name="success" data-com-type="wx" bind:__l="__l"></van-icon></block><van-button class="btn1" vue-id="{{'3-'+index}}" data-index="{{index}}" size="mini" type="default" wx:else data-event-opts="{{[['^tap',[['use']]]]}}" data-com-type="wx" bind:tap="__e" bind:__l="__l" vue-slots="{{['default']}}">使用</van-button></view></view></view><view class="location_bottom"><view class="b_left"><icon class="{{[item.def?'iconfont icon-xuanzhong-copy':'iconfont icon-xuanzhongyuandian']}}" data-index="{{index}}" data-event-opts="{{[['tap',[['ctrl',['$event']]]]]}}" bindtap="__e"></icon><text class="{{[item.def?'red':'']}}">{{item.def?text:text1}}</text></view><view class="b_right"><view class="edit">编辑</view><view class="topping" data-index="{{index}}" data-event-opts="{{[['tap',[['toTop',['$event']]]]]}}" bindtap="__e">{{item.top?top:top1}}</view></view></view></view></block></block><van-button class="button" vue-id="4" size="large" type="danger" data-event-opts="{{[['^tap',[['add']]]]}}" data-com-type="wx" bind:tap="__e" bind:__l="__l" vue-slots="{{['default']}}"><image src="../../static/img/add.png"></image><text>添加新收货地址</text></van-button></view></view>